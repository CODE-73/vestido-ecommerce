{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@vestido-ecommerce/auth": [
        "libs/auth/src/index.ts",
        "../../libs/auth/src/index.ts"
      ],
      "@vestido-ecommerce/auth/client": [
        "libs/auth/src/client.ts",
        "../../libs/auth/src/client.ts"
      ],
      "@vestido-ecommerce/caching": [
        "libs/caching/src/index.ts",
        "../../libs/caching/src/index.ts"
      ],
      "@vestido-ecommerce/fast2sms": [
        "third-party/fast2sms/src/index.ts",
        "../../third-party/fast2sms/src/index.ts"
      ],
      "@vestido-ecommerce/items": [
        "libs/items/src/index.ts",
        "../../libs/items/src/index.ts"
      ],
      "@vestido-ecommerce/items/client": [
        "libs/items/src/client.ts",
        "../../libs/items/src/client.ts"
      ],
      "@vestido-ecommerce/models": [
        "libs/models/src/index.ts",
        "../../libs/models/src/index.ts"
      ],
      "@vestido-ecommerce/orders": [
        "libs/orders/src/index.ts",
        "../../libs/orders/src/index.ts"
      ],
      "@vestido-ecommerce/orders/client": [
        "libs/orders/src/client.ts",
        "../../libs/orders/src/client.ts"
      ],
      "@vestido-ecommerce/posthog": [
        "third-party/posthog/src/index.ts",
        "../../third-party/posthog/src/index.ts"
      ],
      "@vestido-ecommerce/posthog/client": [
        "third-party/posthog/src/client.ts",
        "../../third-party/posthog/src/client.ts"
      ],
      "@vestido-ecommerce/r2": [
        "libs/r2/src/index.ts",
        "../../libs/r2/src/index.ts"
      ],
      "@vestido-ecommerce/razorpay": [
        "libs/razorpay/src/index.ts",
        "../../libs/razorpay/src/index.ts"
      ],
      "@vestido-ecommerce/shadcn-ui": [
        "libs/shadcn-ui/src/index.ts",
        "../../libs/shadcn-ui/src/index.ts"
      ],
      "@vestido-ecommerce/shadcn-ui/*": [
        "libs/shadcn-ui/src/ui/*",
        "../../libs/shadcn-ui/src/ui/*"
      ],
      "@vestido-ecommerce/utils": [
        "libs/utils/src/index.ts",
        "../../libs/utils/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
