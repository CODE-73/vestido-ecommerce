{
  "name": "vestido-storefront",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "vestido/vestido-storefront",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "dependsOn": [
        "models:generate-types",
        "shadcn-ui:build"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/vestido/vestido-storefront"
      },
      "configurations": {
        "development": {
          "outputPath": "vestido/vestido-storefront"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "dependsOn": [
        "models:generate-types",
        "shadcn-ui:build"
      ],
      "options": {
        "buildTarget": "vestido-storefront:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "vestido-storefront:build:development",
          "dev": true,
          "port": 4200
        },
        "production": {
          "buildTarget": "vestido-storefront:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "dependsOn": [
        "models:generate-types",
        "shadcn-ui:build"
      ],
      "options": {
        "buildTarget": "vestido-storefront:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "dependsOn": [
        "models:generate-types",
        "shadcn-ui:build"
      ],
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "vestido/vestido-storefront/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": [
        "{options.outputFile}"
      ]
    },
    "cap": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "--help"
      }
    },
    "add": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "add"
      },
      "configurations": {
        "ios": {
          "cmd": "add ios"
        },
        "android": {
          "cmd": "add android"
        }
      }
    },
    "copy": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "copy"
      },
      "configurations": {
        "ios": {
          "cmd": "copy ios"
        },
        "android": {
          "cmd": "copy android"
        }
      }
    },
    "open": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "open"
      },
      "configurations": {
        "ios": {
          "cmd": "open ios"
        },
        "android": {
          "cmd": "open android"
        }
      }
    },
    "run": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "run"
      },
      "configurations": {
        "ios": {
          "cmd": "run ios"
        },
        "android": {
          "cmd": "run android"
        }
      }
    },
    "sync": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "sync"
      },
      "configurations": {
        "ios": {
          "cmd": "sync ios"
        },
        "android": {
          "cmd": "sync android"
        }
      }
    },
    "update": {
      "executor": "@nxext/capacitor:cap",
      "options": {
        "cmd": "update"
      },
      "configurations": {
        "ios": {
          "cmd": "update ios"
        },
        "android": {
          "cmd": "update android"
        }
      }
    }
  }
}